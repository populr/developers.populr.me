{
  "source": "./src",
  "metadata": {
    "title": "Populr Developer Center",
    "description": "API and Theming Documentation for Populr.me"
  },
  "plugins": {
    "metalsmith-clean": {},
    "metalsmith-drafts": {},
    "metalsmith-encode-html": {},
    "../../../lib/populr-metalsmith-layout": {},
    "metalsmith-permalinks": {
      "relative": false,
      "pattern": ":title"
    },
    "metalsmith-templates": {
      "engine": "handlebars",
      "directory": "templates"
    }
  }
}